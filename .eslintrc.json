{
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "rules": {
    "indent": [1, 2],
    "semi": [1, "always"],
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn",
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 0 }]
  }
}
